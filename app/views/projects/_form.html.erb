<%= form_with model: @project, local: true do |f| %>
  <%= render partial: 'shared/error_messages', locals: {resource: @project} %>

  <div class='form-group'>
    <%= f.label :title %>
    <%= f.text_field :title, class: 'form-control' %>
  </div>

  <div>
    <%= f.submit class: 'btn' %>
  </div>
<% end %>
